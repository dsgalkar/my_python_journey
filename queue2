class queue:
        def __init__(self,size):
            self.q=[0]
            self.n=size
            self.front=-1
            self.rear=-1
        def isempty(self):
            if self.front==-1 and self.rear==-1:
                return True
            else:
                return False
        def isfull(self):
            if self.front==self.n-1:
                return True
            else:
                return False
        def enqueue(self,x):
            if self.isfull():
                print("Queue is full")
            else:
                if self.isempty():
                    self.front=1
                    self.rear=1
                    self.q[self.rear]=x
                else:
                    self.rear+=1
                    self.q[self.rear]=x
        def dequeue(self):
            if self.isempty():
                print("Queue is empty")
            else:
                if self.front==self.rear:
                    self.front-= 1
                    self.rear-= 1
                else:
                    self.front=self.front+1
#size=int(input("enter the size :- "))
print("is the queue is full  : ")
isfull()
print("is the queue is empty : ")
isempty()